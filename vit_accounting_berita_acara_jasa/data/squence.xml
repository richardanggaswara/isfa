<odoo>
	<data noupdate="1">
		<record id="ba_sequence" model="ir.sequence">
			<field name="name">Berita Acara Jasa squence</field>
			<field name="code">vit.accounting_berita_acara_jasa</field>
			<field name="prefix">BA.{{BA}}</field>
			<field name="padding">2</field>
			<field eval="1" name="number_next"/>
			<field eval="1" name="number_increment"/>
		</record>
		<record id="ba_line_sequence" model="ir.sequence">
			<field name="name">Berita Acara Jasa Line squence</field>
			<field name="code">vit.accounting_berita_acara_jasa_line</field>
			<field name="suffix">/ISFA/{{BALINE}}</field>
			<field name="padding">3</field>
			<field eval="1" name="number_next"/>
			<field eval="1" name="number_increment"/>
		</record>
	</data>
</odoo>